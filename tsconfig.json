    "./src/components/Effects/Distortion/Distortion.ts",
    "./src/components/Effects/Effects.ts",
    "./src/components/Effects/Reverb/Reverb.ts",
    "./src/components/File/FileWrapper.ts",
    "./src/components/File/WaveReader.ts",
    "./src/components/File/XHRFileReader.ts",
    "./src/components/Instrument/StreamNode.ts",
    "./src/components/Mixer/Mixer.ts",
    "./src/components/Mixer/MixerWindow.tsx",
    "./src/components/Mixer/Track.ts",
    "./src/components/Mixer/TrackWindow.tsx",
