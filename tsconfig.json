    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": true,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": false,
    "suppressImplicitAnyIndexErrors": false,
    "jsx": "react"
